    proc = new Proc();
    proc.isData = true;
    Line l1 = new Line(t.image.trim());
    proc.lines.addElement(l1);
  }
  ( LOOKAHEAD(2) t = <DATALINE>
    {
      Line l2 = new Line(t.image.trim());
      proc.lines.addElement(l2);
    }
  )*
}

