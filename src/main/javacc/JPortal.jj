}

TOKEN :          // Identifiers
{
  <IDENTIFIER: (<LETTER>|<EXTRA>) (<LETTER>|<DIGIT>|<EXTRA>)*>
| <LIDENTIFIER: ("l"|"L") "'" (<LETTER>|<EXTRA>|<LITERAL>) (<LETTER>|<DIGIT>|<EXTRA>|<LITERAL>)* "'">
| <#LETTER: ["a"-"z", "_", "A"-"Z"]>
| <#DIGIT:  ["0"-"9"]>
| <#EXTRA:  ["#", "$"]>
| <#LITERAL:["[", "]", "\'", "\"", " ","\\"]>
}

